// Printing of array by two different meathods

# include <iostream>
# include <array>
using namespace std;

int main(){
    
    int array1[] = {10,20,30,40,50};
    int array2[5];
    array2[0] = 60 ;
    array2[1] = 70 ;
    array2[2] = 80 ;
    array2[3] = 90 ;
    
    cout << "This is the 1st array " << endl;
    
    // printing of first array
    for(int i = 0;i < 4 ; i++){
        cout << "The value is " << i << endl;
    }
    cout << "This is the 2nd array " << endl;
    // Printing of second array
    cout << array2[0]  << endl;
    cout << array2[1]  << endl ;
    cout << array2[2]  << endl ;
    cout << array2[3]  << endl ;
    
    return 0;
    
}
